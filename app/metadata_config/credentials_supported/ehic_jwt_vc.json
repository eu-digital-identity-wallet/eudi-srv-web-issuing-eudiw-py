{
  "eu.europa.ec.eudi.ehic_sd_jwt_vc":{
     "format":"dc+sd-jwt",
     "scope":"eu.europa.ec.eudi.ehic_sd_jwt_vc",
     "cryptographic_binding_methods_supported":[
        "jwk",
        "cose_key"
     ],
     "credential_signing_alg_values_supported":[
        "ES256"
     ],
     "proof_types_supported":{
        "jwt":{
           "proof_signing_alg_values_supported":[
              "ES256"
           ]
        }
     },
      "vct":"urn:eudi:ehic:1",
      "credential_metadata": {
     "display":[
        {
           "name":"EHIC (SD-JWT VC)",
           "locale":"en",
           "logo":{
              "uri":"https://examplestate.com/public/pid.png",
              "alt_text":"A square figure of a PID"
           }
        }
     ],
     "claims":[
        {
           "path":[
              "personal_administrative_number"
           ],
           "mandatory":true,
           "value_type":"string",
           "source":"user",
           "display":[
              {
                 "name":"Personal Administrative Number",
                 "locale":"en"
              }
           ]
        },
        {
           "path":[
              "issuing_authority"
           ],
           "mandatory":true,
           "source":"issuer",
           "display":[
              {
                 "name":"Issuing Authority",
                 "locale":"en"
              }
           ]
        },
        {
           "path":[
              "issuing_authority", "id"
           ],
           "mandatory":true,
           "source":"issuer",
           "display":[
              {
                 "name":"Issuing Authority ID",
                 "locale":"en"
              }
           ]
        },
        {
           "path":[
              "issuing_authority", "name"
           ],
           "mandatory":true,
           "source":"issuer",
           "display":[
              {
                 "name":"Issuing Authority Name",
                 "locale":"en"
              }
           ]
        },
        {
           "path":[
              "issuing_country"
           ],
           "mandatory":true,
           "source":"issuer",
           "display":[
              {
                 "name":"Issuing Country",
                 "locale":"en"
              }
           ]
        },
         {
           "path":[
              "date_of_expiry"
           ],
           "mandatory":true,
           "source":"issuer",
           "display":[
              {
                 "name":"Expiry date",
                 "locale":"en"
              }
           ]
        },
        {
           "path":[
              "date_of_issuance"
           ],
           "mandatory":true,
           "source":"issuer",
           "display":[
              {
                 "name":"Issue date",
                 "locale":"en"
              }
           ]
        },
        {
           "path":[
              "authentic_source"
           ],
           "mandatory":true,
           "value_type":"authentic_source_attributes",
           "source":"user",
           "display":[
              {
                 "name":"Authentic Source",
                 "locale":"en"
              }
           ],
           "issuer_conditions":{
              "cardinality":{
                 "min":1,
                 "max":1
              }
           }
        },
        {
           "path":[
              "authentic_source",
              "id"
           ],
           "mandatory":true,
           "value_type":"string",
           "source":"user",
           "display":[
              {
                 "name":"Authentic Source ID",
                 "locale":"en"
              }
           ]
        },
        {
           "path":[
              "authentic_source",
              "name"
           ],
           "mandatory":true,
           "value_type":"string",
           "source":"user",
           "display":[
              {
                 "name":"Authentic Source Name",
                 "locale":"en"
              }
           ]
        },
        {
           "path":[
              "starting_date"
           ],
           "mandatory":true,
           "value_type":"full-date",
           "source":"user",
           "display":[
              {
                 "name":"Starting Date",
                 "locale":"en"
              }
           ]
        },
        {
           "path":[
              "ending_date"
           ],
           "mandatory":true,
           "value_type":"full-date",
           "source":"user",
           "display":[
              {
                 "name":"Ending Date",
                 "locale":"en"
              }
           ]
        }
     ]
     },
     "issuer_config":{
        "issuing_authority":"Test QEAA Issuer",
        "issuing_authority_id":"issuer12345",
        "organization_id":"EUDI Wallet Reference Implementation",
        "validity":90,
        "organization_name":"Test QEAA Issuer",
        "doctype":"eu.europa.ec.eudi.ehic.1"
     }
  }
}